{
  "hosting": {
    "site": "hundred-game-d6aaf",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/graphql",
        "run": {
          "serviceId": "hundred-game-backend",
          "region": "us-central1"
        }
      },
      {
        "source": "/game",
        "run": {
          "serviceId": "hundred-game",
          "region": "us-central1"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}